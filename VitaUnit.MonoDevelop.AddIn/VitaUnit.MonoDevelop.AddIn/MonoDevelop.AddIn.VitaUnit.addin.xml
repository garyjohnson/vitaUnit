<AddIn 
	id="VitaUnit" 
	name="VitaUnit" 
	namespace="MonoDevelop.AddIn"
	author="Gary Johnson" 
	copyright="" 
	description="VitaUnit addin for MonoDevelop" 
	category="IDE extensions" 
	version="0.0.1">
	
	<Dependencies>  
    	<Addin id="::MonoDevelop.Ide" version="2.8.8.4"/>  
    	<Addin id="::MonoDevelop.Core" version="2.8.8.4"/>  
  	</Dependencies> 
  	
  	<Extension path = "/MonoDevelop/Ide/Commands/Edit">  
    	<Command id = "MonoDevelop.AddIn.VitaUnit.VitaUnitCommands.RunTests"  
             _label = "Run Tests"  
             defaultHandler = "MonoDevelop.AddIn.VitaUnit.RunTestsHandler"  
             _description = "Run tests" />  
	</Extension>  
  
    <Extension path = "/MonoDevelop/Ide/MainMenu/Edit">  
        <CommandItem id="MonoDevelop.AddIn.VitaUnit.VitaUnitCommands.RunTests" />  
    </Extension>  
</AddIn>